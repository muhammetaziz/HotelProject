@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">
@await Component.InvokeAsync("_UserLayoutHeaderComponent") 
<body>
    <div class="offcanvas-menu-overlay"></div>
    <div class="canvas-open">
        <i class="icon_menu"></i>
    </div>
    @await Component.InvokeAsync("_UserLayoutMenuComponent")
    @await Component.InvokeAsync("_UserLayoutTopHeaderComponent")
    @RenderBody()
    @await Component.InvokeAsync("_UserLayoutFooterComponent")
    <div class="search-model">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="search-close-switch"><i class="icon_close"></i></div>
            <form class="search-model-form">
                <input type="text" id="search-input" placeholder="Search here.....">
            </form>
        </div>
    </div>
    <script src="~/sona-master/js/jquery-3.3.1.min.js"></script>
    <script src="~/sona-master/js/bootstrap.min.js"></script>
    <script src="~/sona-master/js/jquery.magnific-popup.min.js"></script>
    <script src="~/sona-master/js/jquery.nice-select.min.js"></script>
    <script src="~/sona-master/js/jquery-ui.min.js"></script>
    <script src="~/sona-master/js/jquery.slicknav.js"></script>
    <script src="~/sona-master/js/owl.carousel.min.js"></script>
    <script src="~/sona-master/js/main.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js" integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
</body>

</html>